{
    "name": "project-one-companion",
    "version": "2.5.0",
    "description": "Project One guild companion app",
    "main": "./out/main/index.js",
    "author": "Project One Devs",
    "homepage": "https://github.com/project-one-developers/project-one-companion",
    "repository": {
        "type": "git",
        "url": "https://github.com/project-one-developers/project-one-companion.git"
    },
    "scripts": {
        "format": "prettier \"**/*\" --write --ignore-unknown --cache",
        "lint": "eslint . && tsc --noEmit -p tsconfig.node.json --composite false && tsc --noEmit -p tsconfig.web.json --composite false",
        "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
        "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
        "typecheck": "npm run typecheck:node && npm run typecheck:web",
        "start": "electron-vite preview",
        "dev": "electron-vite dev -w",
        "build": "npm run lint && electron-vite build",
        "build:win": "npm run build && electron-builder --config electron-builder-config.ts --win --x64 --arm64",
        "build:mac": "npm run build && electron-builder --config electron-builder-config.ts --mac",
        "build:linux": "npm run build && electron-builder --config electron-builder-config.ts --linux",
        "postinstall": "electron-builder install-app-deps",
        "publish:dev": "dotenv -e .env.development -- electron-builder --config electron-builder-config.ts --publish always --config.publish.provider=s3 --config.publish.endpoint=http://localhost:9000 --config.publish.bucket=test-update",
        "migratedb": "drizzle-kit push --verbose",
        "migratedb-prod": "dotenv -e .env.production -- drizzle-kit push",
        "test": "jest --passWithNoTests",
        "clean": "rimraf ./out && rimraf ./node_modules"
    },
    "dependencies": {
        "@electron-toolkit/preload": "3.0.1",
        "@electron-toolkit/utils": "4.0.0",
        "@hookform/resolvers": "4.1.3",
        "@radix-ui/react-checkbox": "1.1.4",
        "@radix-ui/react-collapsible": "1.1.3",
        "@radix-ui/react-dropdown-menu": "2.1.6",
        "@radix-ui/react-label": "2.1.2",
        "@radix-ui/react-popover": "1.1.6",
        "@radix-ui/react-select": "2.1.6",
        "@radix-ui/react-separator": "1.1.2",
        "@radix-ui/react-tabs": "1.1.3",
        "@radix-ui/react-toast": "1.2.6",
        "@radix-ui/react-toggle-group": "1.1.2",
        "@radix-ui/react-tooltip": "1.1.8",
        "@tanstack/react-query": "5.68.0",
        "class-variance-authority": "0.7.1",
        "clsx": "2.1.1",
        "cmdk": "1.1.1",
        "discord.js": "14.18.0",
        "drizzle-kit": "0.30.5",
        "drizzle-orm": "0.40.0",
        "electron-updater": "6.3.9",
        "file-saver": "^2.0.5",
        "framer-motion": "12.5.0",
        "lucide-react": "0.482.0",
        "p-limit": "^6.2.0",
        "papaparse": "^5.5.2",
        "postgres": "3.4.5",
        "react-hook-form": "7.54.2",
        "react-router": "7.3.0",
        "react-router-dom": "7.3.0",
        "ts-pattern": "5.6.2",
        "uuid": "11.1.0",
        "winston": "3.17.0",
        "zod": "3.24.2"
    },
    "devDependencies": {
        "@types/node": "22.13.10",
        "@types/react": "19.0.10",
        "@types/react-dom": "19.0.4",
        "@vitejs/plugin-react": "4.3.4",
        "autoprefixer": "10.4.21",
        "dotenv": "^16.4.7",
        "dotenv-cli": "8.0.0",
        "electron": "35.0.2",
        "electron-builder": "25.1.8",
        "electron-log": "5.3.2",
        "electron-store": "10.0.1",
        "electron-vite": "3.0.0",
        "eslint": "9.22.0",
        "eslint-config-prettier": "10.1.1",
        "eslint-plugin-react": "7.37.4",
        "eslint-plugin-react-hooks": "5.2.0",
        "globals": "16.0.0",
        "jest": "29.7.0",
        "postcss": "8.5.3",
        "prettier": "3.5.3",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "rimraf": "6.0.1",
        "tailwind-merge": "3.0.2",
        "tailwindcss": "3.4.17",
        "tailwindcss-animate": "1.0.7",
        "ts-jest": "29.2.6",
        "typescript": "5.8.2",
        "typescript-eslint": "8.26.1",
        "vite": "6.2.3"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "electron",
            "esbuild"
        ]
    },
    "packageManager": "pnpm@10.6.2+sha512.47870716bea1572b53df34ad8647b42962bc790ce2bf4562ba0f643237d7302a3d6a8ecef9e4bdfc01d23af1969aa90485d4cebb0b9638fa5ef1daef656f6c1b"
}
